      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{786C4F2E-CD6D-4D97-8A45-62C779F89A91}"
          SQLTask:SqlStatementSource="DECLARE @StartDate DATE = '2016-01-01';&#xA;DECLARE @EndDate   DATE = '2030-12-31';&#xA;DECLARE @current_date DATE = @StartDate;&#xA;&#xA;WHILE @current_date &lt;= @EndDate&#xA;BEGIN&#xA;    INSERT INTO DimDate (&#xA;        date_key,&#xA;        date,&#xA;        year,&#xA;        quarter,&#xA;        month,&#xA;        month_name,&#xA;        day,&#xA;        day_of_week,&#xA;        day_name,&#xA;        week,&#xA;        is_weekend,&#xA;        is_holiday&#xA;    )&#xA;    VALUES (&#xA;        CONVERT(INT, FORMAT(@current_date, 'yyyyMMdd')),       -- date_key&#xA;        @current_date,                                         -- date&#xA;        YEAR(@current_date),                                   -- year&#xA;        DATEPART(QUARTER, @current_date),                      -- quarter&#xA;        MONTH(@current_date),                                  -- month&#xA;        DATENAME(MONTH, @current_date),                        -- month_name&#xA;        DAY(@current_date),                                    -- day&#xA;        DATEPART(WEEKDAY, @current_date),                      -- day_of_week&#xA;        DATENAME(WEEKDAY, @current_date),                      -- day_name&#xA;        DATEPART(WEEK, @current_date),                         -- week&#xA;        CASE &#xA;            WHEN DATEPART(WEEKDAY, @current_date) IN (1,7) THEN 1 &#xA;            ELSE 0 &#xA;        END,                                                   -- is_weekend&#xA;        0                                                      -- is_holiday (default)&#xA;    );&#xA;&#xA;    SET @current_date = DATEADD(DAY, 1, @current_date);&#xA;END;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
