  <DataSets>
    <DataSet Name="Clientes">
      <Query>
        <DataSourceName>BikeStoresDW</DataSourceName>
        <CommandText>SELECT DISTINCT
 
    c.customer_key, 
 
    c.full_name
FROM DimCustomer AS c

WHERE c.EndDate IS NULL

ORDER BY c.full_name;</CommandText>
      </Query>
      <Fields>
        <Field Name="customer_key">
          <DataField>customer_key</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="full_name">
          <DataField>full_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Marcas">
      <Query>
        <DataSourceName>BikeStoresDW</DataSourceName>
        <CommandText>SELECT DISTINCT
 
    dp.brand_id,

    dp.brand_name

FROM DimProduct AS dp

ORDER BY dp.brand_name;</CommandText>
      </Query>
      <Fields>
        <Field Name="brand_id">
          <DataField>brand_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="brand_name">
          <DataField>brand_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Ventas">
      <Query>
        <DataSourceName>BikeStoresDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CustomerKey">
            <Value>=Parameters!CustomerKey.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@BrandId">
            <Value>=Parameters!BrandId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT

    dp.category_name,

    dp.product_name,

    SUM(fs.quantity) AS total_quantity,

    SUM(fs.net_sales_amount) AS total_sales

FROM FactSales AS fs

INNER JOIN DimProduct AS dp
 ON fs.product_key = dp.product_key

INNER JOIN DimCustomer AS dc
 ON fs.customer_key = dc.customer_key

INNER JOIN DimDate AS dd
 ON fs.order_date_key = dd.date_key

WHERE
    (@CustomerKey IS NULL OR fs.customer_key = @CustomerKey)

    AND (@BrandId IS NULL OR dp.brand_id = @BrandId)

    AND (@StartDate IS NULL OR dd.[date] &gt;= @StartDate)

    AND (@EndDate IS NULL OR dd.[date] &lt;= @EndDate)

GROUP BY

    dp.category_name,

    dp.product_name

ORDER BY

    dp.category_name,

    dp.product_name;</CommandText>
      </Query>
      <Fields>
        <Field Name="category_name">
          <DataField>category_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="product_name">
          <DataField>product_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="total_quantity">
          <DataField>total_quantity</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="total_sales">
          <DataField>total_sales</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
