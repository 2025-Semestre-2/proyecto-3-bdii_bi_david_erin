  <DataSets>
    <DataSet Name="Sucursales">
      <Query>
        <DataSourceName>BikeStores</DataSourceName>
        <CommandText>SELECT store_id, store_name

FROM sales.stores

ORDER BY store_name;</CommandText>
      </Query>
      <Fields>
        <Field Name="store_id">
          <DataField>store_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="store_name">
          <DataField>store_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Categorias">
      <Query>
        <DataSourceName>BikeStores</DataSourceName>
        <CommandText>SELECT NULL AS category_id, '&lt;Todas&gt;' AS category_name

UNION ALL

SELECT category_id, category_name

FROM production.categories

ORDER BY category_name;</CommandText>
      </Query>
      <Fields>
        <Field Name="category_id">
          <DataField>category_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="category_name">
          <DataField>category_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Marcas">
      <Query>
        <DataSourceName>BikeStores</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CategoryId">
            <Value>=Parameters!CategoryId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT NULL AS brand_id, '&lt;Todas&gt;' AS brand_name

UNION ALL

SELECT DISTINCT
 
    b.brand_id, 
 
    b.brand_name

FROM production.brands b
JOIN production.products p
 ON p.brand_id = b.brand_id
WHERE
 
    (@CategoryId IS NULL OR p.category_id = @CategoryId)
ORDER BY brand_name;</CommandText>
      </Query>
      <Fields>
        <Field Name="brand_id">
          <DataField>brand_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="brand_name">
          <DataField>brand_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Inventario">
      <Query>
        <DataSourceName>BikeStores</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CategoryId">
            <Value>=Parameters!CategoryId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@BrandId">
            <Value>=Parameters!BrandId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StoreId">
            <Value>=Parameters!StoreId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
 
   p.product_id,
 
    p.product_name,

    b.brand_name,
 
    SUM(stk.quantity) AS total_inventory

FROM production.products AS p

INNER JOIN production.brands AS b ON p.brand_id = b.brand_id

INNER JOIN production.stocks AS stk ON p.product_id = stk.product_id

WHERE
 
    (@CategoryId IS NULL OR p.category_id = @CategoryId)
    
    AND (@BrandId IS NULL OR p.brand_id = @BrandId)
   
    AND (@StoreId IS NULL OR stk.store_id = @StoreId)

GROUP BY 

    p.product_id,

    p.product_name,

    b.brand_name

ORDER BY

    b.brand_name,

    p.product_name;</CommandText>
      </Query>
      <Fields>
        <Field Name="product_id">
          <DataField>product_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="product_name">
          <DataField>product_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="brand_name">
          <DataField>brand_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="total_inventory">
          <DataField>total_inventory</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
